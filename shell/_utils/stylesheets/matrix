#!/bin/sh

# . stylesheets/matrix
# set a list of tags based on constants defined in file _utils/colors

COLOR1="${F}${GREEN}"
COLOR2="${F}${BLACK}"
COLOR3="${F}${WHITE}"

TEXT="${PLAIN}${COLOR1}"
STRONG="${BOLD}${COLOR3}"
TITLE="${BOLD}${COLOR1}"
DESC="${TEXT}"
PARAM="${TITLE}"
SECTION="${BOLD}${COLOR2}"
