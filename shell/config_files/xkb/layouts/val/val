xkb_symbols "reversed_symbols" {
    key <AE01> {[   exclam,                     1,                          NoSymbol,                   NoSymbol                    ]}; // !1
    key <AE02> {[   at,                         2,                          NoSymbol,                   NoSymbol                    ]}; // @2
    key <AE03> {[   numbersign,                 3,                          NoSymbol,                   NoSymbol                    ]}; // #3
    key <AE04> {[   dollar,                     4,                          NoSymbol,                   NoSymbol                    ]}; // $4
    key <AE05> {[   percent,                    5,                          NoSymbol,                   NoSymbol                    ]}; // %5
    key <AE06> {[   asciicircum,                6,                          NoSymbol,                   NoSymbol                    ]}; // ^6
    key <AE07> {[   ampersand,                  7,                          NoSymbol,                   NoSymbol                    ]}; // &7
    key <AE08> {[   asterisk,                   8,                          NoSymbol,                   NoSymbol                    ]}; // *8
    key <AE09> {[   parenleft,                  9,                          NoSymbol,                   NoSymbol                    ]}; // (9
    key <AE10> {[   parenright,                 0,                          NoSymbol,                   NoSymbol                    ]}; // )0
};


