xkb_symbols "t1" {
    include "val(basic)"
    include "val(reversed_symbols)"

    name[Group1]= "val English (t1)";

//                  unmodified,                 shift,                      altgr,                      shift+altgr
// high row
    key <TLDE> {[   asciitilde,                 asciicircum,                grave,                      NoSymbol                    ]}; // ^~`
//  key <AE01> {[   exclam,                     1,                          NoSymbol,                   NoSymbol                    ]}; // !1
    key <AE05> {[   bar,                        5,                          NoSymbol,                   NoSymbol                    ]}; // |5
    key <AE06> {[   exclam,                     6,                          NoSymbol,                   NoSymbol                    ]}; // !6
    key <AE07> {[   ampersand,                  7,                          NoSymbol,                   NoSymbol                    ]}; // &7
    key <AE08> {[   parenleft,                  8,                          NoSymbol,                   NoSymbol                    ]}; // *8
    key <AE09> {[   parenright,                 9,                          NoSymbol,                   NoSymbol                    ]}; // 9(
    key <AE10> {[   backslash,                  0,                          NoSymbol,                   NoSymbol                    ]}; // 0)
    key <AE11> {[   minus,                      grave,                      NoSymbol,                   NoSymbol                    ]}; // -_
    key <AE12> {[   equal,                      VoidSymbol,                 VoidSymbol,                 NoSymbol                    ]}; // =
// second row
    key <AD11> {[   braceleft,                  braceright,                 leftsinglequotemark,        rightsinglequotemark        ]}; // {}“”
    key <AD12> {[   bracketleft,                bracketright,               leftdoublequotemark,        rightdoublequotemark        ]}; // []‘’
// third row
    key <AC10> {[   apostrophe,                 quotedbl,                   VoidSymbol,                 VoidSymbol                  ]}; // '"
    key <AC11> {[   semicolon,                  colon,                      VoidSymbol,                 VoidSymbol                  ]}; // ;:
    key <BKSL> {[   underscore,                 question,                   NoSymbol,                   NoSymbol                    ]}; // +
// fourth row
    key <AB10> {[   slash,                      asterisk,                   percent,                    NoSymbol                    ]}; // /*
// f-keys row
    key <PRSC> {[ VoidSymbol ]};
};


