xkb_symbols "basic" {
    include "level3(ralt_switch)"

    name[Group1]= "val English (enhanced qwerty)";

//                  unmodified,                 shift,                      altgr,                      shift+altgr
// high row
    key <TLDE> {[   asciitilde,                 grave,                      NoSymbol,                   NoSymbol                    ]}; // ~`
    key <AE01> {[   1,                          exclam,                     NoSymbol,                   NoSymbol                    ]}; // 1!
    key <AE02> {[   2,                          at,                         NoSymbol,                   NoSymbol                    ]}; // 2@
    key <AE03> {[   3,                          numbersign,                 NoSymbol,                   NoSymbol                    ]}; // 3#
    key <AE04> {[   4,                          dollar,                     EuroSign,                   NoSymbol                    ]}; // 4$€
    key <AE05> {[   5,                          percent,                    NoSymbol,                   NoSymbol                    ]}; // 5%
    key <AE06> {[   6,                          asciicircum,                NoSymbol,                   NoSymbol                    ]}; // 6^
    key <AE07> {[   7,                          ampersand,                  NoSymbol,                   NoSymbol                    ]}; // 7&
    key <AE08> {[   8,                          asterisk,                   NoSymbol,                   NoSymbol                    ]}; // 8*
    key <AE09> {[   9,                          parenleft,                  NoSymbol,                   NoSymbol                    ]}; // 9(
    key <AE10> {[   0,                          parenright,                 NoSymbol,                   NoSymbol                    ]}; // 0)
    key <AE11> {[   minus,                      underscore,                 NoSymbol,                   NoSymbol                    ]}; // -_
    key <AE12> {[   equal,                      plus,                       NoSymbol,                   NoSymbol                    ]}; // =+
// second row
    key <AD01> {[   q,                          Q,                          NoSymbol,                   NoSymbol                    ]}; // qQ
    key <AD02> {[   w,                          W,                          NoSymbol,                   NoSymbol                    ]}; // wW
    key <AD03> {[   e,                          E,                          NoSymbol,                   NoSymbol                    ]}; // eE
    key <AD04> {[   r,                          R,                          NoSymbol,                   NoSymbol                    ]}; // rR
    key <AD05> {[   t,                          T,                          NoSymbol,                   NoSymbol                    ]}; // tT
    key <AD06> {[   y,                          Y,                          NoSymbol,                   NoSymbol                    ]}; // yY
    key <AD07> {[   u,                          U,                          NoSymbol,                   NoSymbol                    ]}; // uU
    key <AD08> {[   i,                          I,                          NoSymbol,                   NoSymbol                    ]}; // iI
    key <AD09> {[   o,                          O,                          NoSymbol,                   NoSymbol                    ]}; // oO
    key <AD10> {[   p,                          P,                          NoSymbol,                   NoSymbol                    ]}; // pP
    key <AD11> {[   bracketleft,                braceleft,                  leftdoublequotemark,        NoSymbol                    ]}; // [{“
    key <AD12> {[   bracketright,               braceright,                 rightdoublequotemark,       NoSymbol                    ]}; // ]}”
// third row
//  key <CAPS> {[   Shift_L,                    Caps_Lock,                  Caps_Lock,                  Caps_Lock                   ]};
    key <AC01> {[   a,                          A,                          NoSymbol,                   NoSymbol                    ]}; // aA
    key <AC02> {[   s,                          S,                          NoSymbol,                   NoSymbol                    ]}; // sS
    key <AC03> {[   d,                          D,                          NoSymbol,                   NoSymbol                    ]}; // dD
    key <AC04> {[   f,                          F,                          NoSymbol,                   NoSymbol                    ]}; // fF
    key <AC05> {[   g,                          G,                          NoSymbol,                   NoSymbol                    ]}; // gG
    key <AC06> {[   h,                          H,                          NoSymbol,                   NoSymbol                    ]}; // hH
    key <AC07> {[   j,                          J,                          NoSymbol,                   NoSymbol                    ]}; // jJ
    key <AC08> {[   k,                          K,                          NoSymbol,                   NoSymbol                    ]}; // kK
    key <AC09> {[   l,                          L,                          NoSymbol,                   NoSymbol                    ]}; // lL
    key <AC10> {[   semicolon,                  colon,                      leftsinglequotemark,        NoSymbol                    ]}; // ;:‘
    key <AC11> {[   apostrophe,                 quotedbl,                   rightsinglequotemark,       NoSymbol                    ]}; // '"’
    key <BKSL> {[   backslash,                  bar,                        NoSymbol,                   NoSymbol                    ]}; // \|
// fourth row
    key <LSGT> {[   NoSymbol,                   NoSymbol,                   VoidSymbol,                 VoidSymbol                  ]}; // 
    key <AB01> {[   z,                          Z,                          NoSymbol,                   NoSymbol                    ]}; // zZ
    key <AB02> {[   x,                          X,                          NoSymbol,                   NoSymbol                    ]}; // xX
    key <AB03> {[   c,                          C,                          NoSymbol,                   NoSymbol                    ]}; // cC
    key <AB04> {[   v,                          V,                          NoSymbol,                   NoSymbol                    ]}; // vV
    key <AB05> {[   b,                          B,                          NoSymbol,                   NoSymbol                    ]}; // bB
    key <AB06> {[   n,                          N,                          asciitilde,                 NoSymbol                    ]}; //
    key <AB07> {[   m,                          M,                          NoSymbol,                   NoSymbol                    ]}; // mM
    key <AB08> {[   comma,                      less,                       NoSymbol,                   NoSymbol                    ]}; // ,<
    key <AB09> {[   period,                     greater,                    NoSymbol,                   NoSymbol                    ]}; // .>
    key <AB10> {[   slash,                      question,                   NoSymbol,                   NoSymbol                    ]}; // /?
