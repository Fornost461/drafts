#!/usr/bin/env sh

# supposed to be launched by i3 from ~/.i3/config when i3 starts

firefox &
spacefm &

sudo ~/bin/luminosity 98

i3-msg "workspace 2; exec st -f 'Source Code Pro' sudo ~/bin/luminosity"
sleep 0.1   # prevent the windows from opening out of sequence
i3-msg "exec st -f 'Source Code Pro' alsamixer"
# (i3â€™s exec command is used so as to make sure that the windows open in the workspaces where they were started from)

exec ~/bin/config_files/session/startup_2.py
